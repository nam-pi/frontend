{
  "name": "nampi-frontend",
  "version": "0.6.5",
  "private": true,
  "dependencies": {
    "//": "TODO: Replace with the original react-leaflet package after this [issue](https://github.com/PaulLeCam/react-leaflet/issues/891) is resolved.",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@headlessui/react": "^1.4.1",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/postcss7-compat": "^2.2.16",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/jsonld": "^1.5.6",
    "@types/leaflet": "^1.7.5",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "^8.3.1",
    "autoprefixer": "^9",
    "babel-plugin-react-intl": "^8.2.25",
    "clsx": "^1.1.1",
    "craco": "^0.0.3",
    "date-fns": "^2.25.0",
    "eslint-plugin-formatjs": "^2.18.0",
    "focus-visible": "^5.2.0",
    "fuse.js": "^6.4.6",
    "husky": "^7.0.4",
    "jsonld": "^5.2.0",
    "keycloak-js": "^15.0.2",
    "leaflet": "^1.7.1",
    "lint-staged": "^11.2.6",
    "nampi-use-api": "^0.6.0",
    "postcss": "^7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.21.0",
    "react-leaflet": "npm:@monsonjeremy/react-leaflet",
    "react-markdown": "^7.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "remark-gfm": "^3.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "extract": "formatjs extract"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "formatjs"
    ],
    "rules": {
      "formatjs/no-id": "error",
      "formatjs/enforce-default-message": [
        "error",
        "literal"
      ],
      "formatjs/enforce-description": [
        "error",
        "literal"
      ],
      "formatjs/enforce-placeholders": "error",
      "formatjs/no-camel-case": "off",
      "formatjs/no-multiple-whitespaces": "error",
      "formatjs/no-offset": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
